<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
	<template>
		<name x="5" y="5">Farming_AV</name>
		<declaration>// Place local declarations here.
</declaration>
		<location id="id0" x="-204" y="195">
			<name x="-297" y="161">Driving</name>
			<label kind="invariant" x="-323" y="187">speed &lt;= 10</label>
		</location>
		<location id="id1" x="518" y="518">
			<name x="544" y="501">Avoiding Obstacles</name>
			<label kind="invariant" x="544" y="527">obstacle_avoidance_time &lt;= 60</label>
		</location>
		<location id="id2" x="501" y="-170">
			<name x="535" y="-196">Identifying Plants</name>
			<label kind="invariant" x="535" y="-170">plant_inspection_time &lt;= 30</label>
		</location>
		<location id="id3" x="-323" y="-93">
			<name x="-484" y="-127">Identifying Weed</name>
			<label kind="invariant" x="-586" y="-101">plant_inspection_time &lt;= 30</label>
		</location>
		<location id="id4" x="-102" y="-102">
			<name x="-85" y="-136">Identifying Deceased</name>
			<label kind="invariant" x="-68" y="-110">plant_inspection_time &lt;= 30</label>
		</location>
		<init ref="id0"/>
		<transition id="id5">
			<source ref="id4"/>
			<target ref="id0"/>
			<label kind="guard" x="-238" y="42">plant_inspetion_time &gt; 30</label>
			<label kind="assignment" x="-204" y="76">resumeDriving()</label>
		</transition>
		<transition id="id6">
			<source ref="id3"/>
			<target ref="id0"/>
			<label kind="guard" x="-467" y="-25">plant_inspetion_time &gt; 30</label>
			<label kind="assignment" x="-408" y="8">resumeDriving()</label>
		</transition>
		<transition id="id7">
			<source ref="id2"/>
			<target ref="id4"/>
			<label kind="guard" x="-153" y="-246">plant_type == disease</label>
			<label kind="assignment" x="-136" y="-212">logPlantData(plant_type)</label>
			<nail x="-221" y="-323"/>
		</transition>
		<transition id="id8">
			<source ref="id2"/>
			<target ref="id3"/>
			<label kind="guard" x="-433" y="-238">plant_type == weed</label>
			<label kind="assignment" x="-510" y="-212">logPlantData(plant_type)</label>
			<nail x="-221" y="-323"/>
		</transition>
		<transition id="id9">
			<source ref="id2"/>
			<target ref="id0"/>
			<label kind="guard" x="-68" y="-34">plant_type != weed &amp;&amp; plant_type != disease</label>
			<label kind="assignment" x="8" y="0">logPlantData(plant_type),
resumeDriving</label>
			<nail x="110" y="0"/>
		</transition>
		<transition id="id10">
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="guard" x="119" y="246">obstacleDistance &lt; 5</label>
			<label kind="assignment" x="34" y="280">obstacleDistance = updateObstaclePosition()</label>
			<nail x="161" y="272"/>
		</transition>
		<transition id="id11">
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="guard" x="-8" y="340">guard obstacleDistance &gt; 5</label>
			<label kind="assignment" x="68" y="391">resumeDriving()</label>
			<nail x="119" y="374"/>
		</transition>
		<transition id="id12">
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="guard" x="569" y="144">obstacleDistance &lt; 5</label>
			<label kind="assignment" x="569" y="178">obstacleDistance = updateObstaclePosition()</label>
			<nail x="569" y="178"/>
		</transition>
		<transition id="id13">
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="guard" x="68" y="42">plantDetected == true</label>
			<label kind="assignment" x="42" y="76">plant_type = classifyPlant()</label>
			<nail x="153" y="59"/>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
// List one or more processes to be composed into a system.
system Process;
</system>
	<queries>
		<query>
			<formula/>
			<comment/>
		</query>
	</queries>
</nta>
